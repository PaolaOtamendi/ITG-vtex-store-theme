{
    "flex-layout.row#global__home-shelf-slide--1": {
        "title": "Container Golbal home shelf slide 1",
        "children": [
            "flex-layout.col#global__home--card-one",
            "list-context.product-list#global__home-shelf--one"
        ],
        "props": {
            "blockClass": [
                "global__home-shelf-slide--1",
                "global__home-shelf-slide"
            ],
            "colSizing": "auto"
        }
    },
    "list-context.product-list#global__home-shelf--one": {
        "title": "Global home shelf one",
        "blocks": [
            "product-summary.shelf#global__shelf--product-summary"
        ],
        "children": [
            "list-context-renderer#global__shelf--render"
        ],
        "props": {
            "blockClass": "global__home-shelf--one"
        }
    },
    "list-context-renderer#global__shelf--render": {
        "props": {
            "listElement": "div",
            "blockClass": "global__shelf--render"
        }
    },
    "product-summary.shelf#global__shelf--product-summary": {
        "title": "Global shelf produc summary one",
        "children": [
            "flex-layout.row#shelf__product-summary--bages",
            "flex-layout.col#shelf__product-summary--image",
            "flex-layout.col#shelf__product-summary-name",
            "flex-layout.col#shelf__product-summary-prices",
            "flex-layout.col#shelf__product-summary-highligths",
            "flex-layout.col#shelf__product-summary-add-to-cart"
        ],
        "props": {
            "width": 176,
            "showDiscountBadge": false,
            "showSavingPercentage": true,
            "blockClass": "global__shelf--product-summary"
        }
    },
    "flex-layout.col#global__home--card-one": {
        "title": "Global home card one",
        "children": [
            "rich-text#home__card-one--text",
            "image#home__card-one--image"
